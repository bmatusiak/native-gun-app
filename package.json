{
  "name": "my-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run android && npm run android:kill && npm run bundler",
    "bundler": "expo start --dev-client -a",
    "android": "expo run:android --no-bundler",
    "android:logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V ReactNativeApp:D",
    "ios" : "echo 'iOS not supported yet'",
    "prebuild": "expo prebuild --clean -p android",
    "android:kill": "node -e \"const pkg=require('./app.json').expo.android.package; require('child_process').execSync('adb shell am force-stop ' + pkg)\"",
    "android:uninstall": "node -e \"const pkg=require('./app.json').expo.android.package; require('child_process').execSync('adb uninstall ' + pkg)\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "app-custom-input": "file:modules/AppCustomInput",
    "keyboard-aware-view": "file:modules/KeyboardAwareView",
    "expo": "~54.0.29",
    "expo-image-picker": "~17.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "native-gun": "github:bmatusiak/native-gun",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0"
  },
  "private": true
}
